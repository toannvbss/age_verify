<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <columns name="customer_columns" class="Magento\Customer\Ui\Component\Listing\Columns">
        <column name="gender" component="Magento_Ui/js/grid/columns/select" sortOrder="190">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="componentDisabled" xsi:type="boolean">true</item>
                </item>
            </argument>
        </column>
        <column name="custom_gender" component="Magento_Ui/js/grid/columns/select" sortOrder="190">
            <settings>
                <options class="Smartosc\Customer\Model\Attribute\CRMGender"/>
                <editor>
                    <editorType>select</editorType>
                </editor>
                <dataType>select</dataType>
                <label translate="true">CRM Gender</label>
                <visible>true</visible>
            </settings>
        </column>
    </columns>
</listing>
