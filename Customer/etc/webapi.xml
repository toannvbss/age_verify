<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route url="/V1/me/address/:addressId" method="DELETE">
        <service class="Smartosc\Customer\Api\Customer\AddressManagementInterface" method="deleteById"/>
        <resources>
            <resource ref="self" />
        </resources>
    </route>
</routes>
